{
    "toolchain": {
        "default": {
            "file_tool_mapping": {
                "cpp": "cxx",
                "c": "cc",
                "S": "as"
            },
            "configuration": {
                "debug": {
                    "cxx": {
                        "flags": {
                            "optimization": "-Og",
                            "defines": "-DDEBUG"
                        }
                    },
                    "cc": {
                        "flags": {
                            "optimization": "-Og",
                            "defines": "-DDEBUG"
                        }
                    }
                },
                "release": {
                    "cxx": {
                        "flags": {
                            "optimization": "-Os",
                            "defines": "-DRELEASE"
                        }
                    },
                    "cc": {
                        "flags": {
                            "optimization": "-Os",
                            "defines": "-DRELEASE"
                        }
                    }
                },
                "minsizerel": {
                },
                "debwithrelinfo": {
                }
            },
            "target_tools": {
                "cxx": {
                    "executable": "i686-pc-serenity-g++",
                    "flags": {
                        "standard": "-MMD -MP -std=c++17 -Wno-sized-deallocation -fno-sized-deallocation",
                        "warning": "-Werror -Wextra -Wall -Wno-nonnull-compare -Wno-deprecated-copy -Wno-address-of-packed-member -Wundef -Wcast-qual -Wwrite-strings -Wimplicit-fallthrough -Wno-expansion-to-defined",
                        "flavor": "-fno-exceptions -fno-rtti -fstack-protector",
                        "optimisation": "-Os",
                        "arch": "",
                        "include": "-I. -IDevTools -ILibraries -IServers -ILibraries/LibC -ILibraries/LibM -ILibraries/LibPthread"
                    }
                },
                "cc": {
                    "executable": "i686-pc-serenity-gcc",
                    "flags": {
                        "standard": "-MMD -MP",
                        "warning": "-Werror -Wextra -Wall -Wno-nonnull-compare -Wno-address-of-packed-member -Wundef -Wcast-qual -Wwrite-strings -Wimplicit-fallthrough -Wno-expansion-to-defined",
                        "flavor": "-fstack-protector",
                        "optimisation": "-Os",
                        "arch": "",
                        "include": "-I. -IDevTools -ILibraries -IServers -ILibraries/LibC -ILibraries/LibM -ILibraries/LibPthread"
                    }
                },
                "as": {
                    "executable": "i686-pc-serenity-as",
                    "flags": ""
                },
                "link": {
                    "executable": "i686-pc-serenity-ld",
                    "flags": ""
                },
                "ranlib": {
                    "executable": "i686-pc-serenity-ranlib",
                    "flags": ""
                },
                "ar": {
                    "executable": "i686-pc-serenity-ar",
                    "flags": ""
                }
            },
            "native_tools": {
                "cxx": {
                    "executable": "g++",
                    "flags": {
                        "standard": "-MMD -MP -std=c++17 -Wno-sized-deallocation -fno-sized-deallocation",
                        "warning": "-Werror -Wextra -Wall -Wno-nonnull-compare -Wno-deprecated-copy -Wno-address-of-packed-member -Wundef -Wcast-qual -Wwrite-strings -Wimplicit-fallthrough -Wno-expansion-to-defined -Wno-unknown-warning-option",
                        "flavor": "-fno-exceptions -fno-rtti -fstack-protector",
                        "optimisation": "-Os",
                        "arch": "",
                        "include": "-I. -IDevTools -ILibraries -IServers"
                    }
                },
                "cc": {
                    "executable": "gcc",
                    "flags": {
                        "standard": "-MMD -MP",
                        "warning": "-Werror -Wextra -Wall -Wno-nonnull-compare -Wno-address-of-packed-member -Wundef -Wcast-qual -Wwrite-strings -Wimplicit-fallthrough -Wno-expansion-to-defined -Wno-unknown-warning-option",
                        "flavor": "-fstack-protector",
                        "optimisation": "-Os",
                        "arch": "",
                        "include": "-I. -IDevTools -ILibraries -IServers"
                    }
                },
                "as": {
                    "executable": "as",
                    "flags": ""
                },
                "link": {
                    "executable": "ld",
                    "flags": ""
                },
                "ranlib": {
                    "executable": "ranlib",
                    "flags": ""
                },
                "ar": {
                    "executable": "ar",
                    "flags": ""
                },
                "download": {
                    "binary": "wget",
                    "flags": "-O-"
                },
                "patch": {
                    "executable": "patch",
                    "flags": ""
                },
                "configure": {
                    "executable": "./configure",
                    "flags": ""
                },
                "make": {
                    "executable": "make",
                    "flags": ""
                },
                "install": {
                    "executable": "make",
                    "flags": "install"
                },
                "cmake": {
                    "executable": "cmake",
                    "flags": ""
                },
                "ninja": {
                    "executable": "ninja",
                    "flags": ""
                },
                "build_qemu": {
                    "executable": "build-qemu-image-linux"
                },
                "run_qemu": {
                    "executable": "qemu-system-i386",
                    "flags": {
                        "standard": "-s -m 256M -cpu max -machine q35 -d cpu_reset,guest_errors -device VGA,vgamem_mb=64 -device piix3-ide",
                        "warning": "-drive file=_disk_image,id=disk,if=none -device ide-hd,bus=ide.6,drive=disk,unit=0 -debugcon stdio",
                        "flavor": "-soundhw pcspk -soundhw sb16 -netdev user,id=breh,hostfwd=tcp:127.0.0.1:8888-10.0.2.15:8888,hostfwd=tcp:127.0.0.1:8823-10.0.2.15:23",
                        "optimisation": "-device e1000,netdev=breh -kernel kernel -append hello"
                    }
                }
            }
        }
    }
}
