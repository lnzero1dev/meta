{
    "package": {
        "AudioServer": {
            "type": "executable",
            "source": [
                "${root}/Servers/AudioServer/*.cpp"
            ],
            "include": [
                "${root}/Servers/AudioServer",
                "${gendata}/AudioServer"
            ],
            "dependency": [
                "LibCore",
                "LibIPC",
                "LibThread",
                "LibPthread"
            ],
            "run_generators": {
                "IPCCompiler": {
                    "input": [
                        "${root}/Servers/AudioServer/AudioClient.ipc",
                        "${root}/Servers/AudioServer/AudioServer.ipc"
                    ]
                }
            },
            "deploy": [
                {
                    "type": "directory",
                    "source": "${root}/Servers/AudioServer/",
                    "pattern": "*.h",
                    "dest": "${IncludeDir}/AudioServer"
                }
            ]
        }
    }
}