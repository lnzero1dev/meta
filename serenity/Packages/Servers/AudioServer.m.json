{
    "package": {
        "AudioServer": {
            "type": "executable",
            "source": [
                "${root}/Servers/AudioServer/*.cpp"
            ],
            "include": [
                "${root}/Servers/AudioServer",
                "${gendata}"
            ],
            "dependency": [
                "LibIPC",
                "LibThread"
            ],
            "run_generators": {
                "IPCCompiler": [
                    {
                        "input": "${root}/Servers/AudioServer/AudioClient.ipc",
                        "output": "${gendata}/AudioClientEndpoint.h"
                    },
                    {
                        "input": "${root}/Servers/AudioServer/AudioServer.ipc",
                        "output": "${gendata}/AudioServerEndpoint.h"
                    }
                ]
            },
            "deploy": [
                {
                    "type": "directory",
                    "source": "${root}/Servers/AudioServer/",
                    "pattern": "*.h",
                    "dest": "${IncludeDir}/AudioServer"
                }
            ]
        }
    }
}
