{
    "package": {
        "FontEditor": {
            "type": "executable",
            "source": [
                "${root}/Applications/FontEditor/*.cpp"
            ],
            "include": [
                "${root}/Applications/FontEditor",
                "${gendata}/FontEditor"
            ],
            "dependency": [
                "LibGUI",
                "LibDraw",
                "LibIPC",
                "LibCore"
            ],
            "run_generators": {
                "FormCompiler": {
                    "input": "FontEditorBottom.frm"
                }
            }
        }
    }
}